<%- model_class = TmpGroup -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:nombre_proyecto) %>:</strong></dt>
  <dd><%= @tmp_group.nombre_proyecto %></dd>
  <dt><strong><%= model_class.human_attribute_name(:users_hash) %>:</strong></dt>
  <dd><%= @tmp_group.users_hash %></dd>
  <dt><strong><%= model_class.human_attribute_name(:activities_hash) %>:</strong></dt>
  <dd><%= @tmp_group.activities_hash %></dd>
  <dt><strong><%= model_class.human_attribute_name(:estado) %>:</strong></dt>
  <dd><%= @tmp_group.estado %></dd>
</dl>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              tmp_groups_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_tmp_group_path(@tmp_group), :class => 'btn' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              tmp_group_path(@tmp_group),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
