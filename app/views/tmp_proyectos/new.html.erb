<%- model_class = TmpProyecto -%>
<div class="container">
  <div class="page-header">
    <h1>Inscribir Proyecto</h1>
  </div>
  <div class="forms_proyecto">
    <%= render partial: 'form' %>
    <%= render partial: 'form_actividad' %>
  </div>

  <div class="form-actions-override">
    <button id="inscribir" type="button" class="btn btn-primary">Inscribir</button>
    <%= link_to t('.cancel', :default => 'Cancelar'),
                tmp_proyectos_path, :class => 'btn' %>
  </div>
</div>

<script type="text/javascript">
  $(function(){
    TmpProyecto.init();
  });
</script>