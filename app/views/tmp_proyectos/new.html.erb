<%- model_class = TmpProyecto -%>
<div class="container">
  <div class="page-header">
    <h1>Inscribir Proyecto</h1>
  </div>
  <div class="forms_proyecto">
    <%= render partial: 'form', locals: {action: crear_solicitud_path, editar: false} %>
  </div>

  <div class="form-actions-override">
    <button id="inscribir" type="button" class="btn btn-primary">Inscribir</button>
    <%= link_to 'Cancelar', solicitudes_path, class: 'btn btn-danger' %>
  </div>
</div>

<script type="text/javascript">
  $(function(){
    TmpProyecto.init();
  });
</script>